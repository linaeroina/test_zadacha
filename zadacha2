import pandas as pd

# Данные по активностям и ролям
activities = ["Инициация проекта", "Лабораторные испытания", ...]
stakeholders = ["Директор", "Иванов И.И.", ...]

# Словарь RACI (пример для одной активности)
raci_data = {
    "Инициация проекта": {"Директор": "A", "Дарья (HR)": "R", ...},
    "Лабораторные испытания": {"Иванов И.И.": "R", ...},
}

# Создание DataFrame
df = pd.DataFrame(raci_data).T.fillna("I")
print(df)
